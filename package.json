{
  "name": "rawgserver",
  "version": "1.0.0",
  "description": "This project is based on an MVC model using a RAWG API based on online games, as well as the creation and maintenance of new games created by users who visit the page.",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./build",
    "start": "node build/index",
    "installese": "npm i && tsc",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules src/index.ts"
  },
  "keywords": [],
  "author": "@SergioAi",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "4.17.21",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "40.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "prettier": "3.1.0",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "tscpaths": "0.0.9",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "dotenv": "16.3.1",
